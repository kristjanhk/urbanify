<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.Region?>
<BorderPane prefHeight="900.0" prefWidth="1600.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="system.graphics.eventManager.Controller">
    <top>



        <HBox>
            <Text styleClass="topText" strokeType="OUTSIDE" strokeWidth="0.0" text="events" BorderPane.alignment="TOP_LEFT">
                <BorderPane.margin>
                    <Insets bottom="40.0" left="40.0" top="40.0"/>
                </BorderPane.margin>
            </Text>
            <Button maxWidth="121.0" minWidth="121.0" mnemonicParsing="false" prefWidth="121.0" text="add"
                    onAction="#doTest">
            </Button>
        </HBox>




    </top>
    <bottom>
        <Button maxWidth="121.0" minWidth="121.0" mnemonicParsing="false" prefWidth="121.0" text="back"
                onAction="#doBack"
                BorderPane.alignment="BOTTOM_RIGHT">
            <BorderPane.margin>
                <Insets bottom="40.0" right="40.0" top="40"/>
            </BorderPane.margin>
        </Button>
    </bottom>
    <center>
        <ScrollPane fitToHeight="true" fitToWidth="true" fx:id="scrollPane" pannable="true" vbarPolicy="AS_NEEDED"
                    BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets left="40.0" right="40.0"/>
            </BorderPane.margin>
            <VBox fx:id="eventsVBox" styleClass="background" prefHeight="739.0" prefWidth="1520.0">
            </VBox>
        </ScrollPane>
    </center>
</BorderPane>
